{
  "experimental": false,
  "features": {
    "buildkit": true
  },
  "builder": {
    "gc": {
      "defaultKeepStorage": "20GB",
      "enabled": true
    }
  },
  "registry-mirrors": [],
  "insecure-registries": [],
  "debug": false,
  "log-level": "info",
  "data-root": "C:\\ProgramData\\Docker\\data",
  "wsl-integration": {
    "enable-distros": ["*"],
    "dns-enabled": true
  },
  "wsl2": {
    "memory": "8GB",
    "processors": 4,
    "swap": "4GB",
    "localhostForwarding": true,
    "kernelCommandLine": "sysctl.vm.max_map_count=262144"
  }
}
